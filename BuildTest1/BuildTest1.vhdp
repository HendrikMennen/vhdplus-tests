Main
(
    led : OUT STD_LOGIC := '0';
    
    BTN : In STD_LOGIC := '0';
)
{
    Process()
    {
        Thread
        {
            led <= '0';
            Wait(1ms);
            if(BTN = '1')
            {
                led <= '1';
                Wait(1ms);
            }          
        }
    }
}